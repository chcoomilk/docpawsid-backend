{
  "name": "docpawsid-backend",
  "version": "1.0.0",
  "description": "server for docpawsid",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.test.json && cross-env TS_NODE_FILES=true tap --ts test/**/*.test.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js\"",
    "database": "npx prisma run dev && npx prisma db seed --preview-feature"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klaustinw/docpawsid-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/klaustinw/docpawsid-backend/issues"
  },
  "homepage": "https://github.com/klaustinw/docpawsid-backend#readme",
  "dependencies": {
    "@prisma/client": "^2.27.0",
    "fastify": "^3.19.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.13.0",
    "fastify-cors": "^6.0.1",
    "fastify-jwt": "^3.0.0",
    "fastify-multipart": "^4.0.7",
    "fastify-oauth2": "^4.2.3",
    "fastify-plugin": "^3.0.0",
    "fastify-rate-limit": "^5.5.0",
    "fastify-sensible": "^3.1.0",
    "fastify-static": "^4.2.2"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.4",
    "@types/node": "^16.3.1",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "prisma": "^2.27.0",
    "tap": "^15.0.9",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
